<#s1client1414>	<name>		"client to 1414" .
<#s1client1414>	<communicateWith>	<#sp1port1414> .

@forSome <#cs1client10060> .
<#cs1client10060>	a <Program> .
<#cs1client10060>	<name>		"client to 10060" .
<#cs1client10060>	<communicateWith>	<#cs1port10060> .

