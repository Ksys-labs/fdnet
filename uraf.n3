#
# clients
#

@forSome <#cryptoapm1client80> .
<#cryptoapm1client80>	a <Program> .
<#cryptoapm1client80>	<name>			"cryptoclient80" .
<#cryptoapm1client80>	<communicateWith>	<#crypto80sp1> .

@forSome <#cryptoapm1client8000> .
<#cryptoapm1client8000>	a <Program> .
<#cryptoapm1client8000>	<name>		"cryptoclient8000" .
<#cryptoapm1client8000>	<communicateWith>	<#crypto8000sp1> .

@forSome <#vpn1apm1client666> .
<#vpn1apm1client666>	a <Program> .
<#vpn1apm1client666>	<name>		"vpn1apm1client666" .
<#vpn1apm1client666>	<communicateWith>	<#vpn1666> .

@forSome <#cs1apm1client666> .
