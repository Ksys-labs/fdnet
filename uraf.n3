@forSome <#crypto8000sp1> .
<#crypto8000sp1>	a <Program> .
<#crypto8000sp1>	<name>		"crypto_server2" .
<#crypto8000sp1>	<listenPort>	"8000" .

@forSome <#22sp1> .
<#22sp1>	a <Program> .
<#22sp1>	<name>		"22 port" .
<#22sp1>	<listenPort>	"22" .

@forSome <#199sp1> .
<#199sp1>	a <Program> .
<#199sp1>	<name>		"199 port" .
<#199sp1>	<listenPort>	"199" .

@forSome <#514sp1> .
<#514sp1>	a <Program> .
