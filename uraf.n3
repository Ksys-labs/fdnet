#
#Servers
#

@forSome <#sp1> .
<#sp1>		a <Server> .
<#sp1>		<name>		"SP1" .
<#sp1>		<ip>		"1.1.1.5" .
<#sp1>		<hasProgram>	<#mysqlsp1> .
<#sp1>		<hasProgram>	<#crypto80sp1> .
<#sp1>		<hasProgram>	<#crypto8000sp1> .
<#sp1>		<hasProgram>	<#514sp1> .
<#sp1>		<hasProgram>	<#199sp1> .
<#sp1>		<hasProgram>	<#22sp1> .
<#sp1>		<hasProgram>	<#sp1port1414> .
<#sp1>		<hasProgram>	<#cs1client10060> .

@forSome <#cs1> .
<#cs1>		a <Server> .
<#cs1>		<name>		"CS1" .
<#cs1>		<ip>		"1.1.1.3" .
<#cs1>		<hasProgram>	<#cs1666> .
<#cs1>		<hasProgram>	<#cs1port10060> .

@forSome <#vpn1> .
<#vpn1>		a <Server> .
<#vpn1>		<name>		"VPN1" .
<#vpn1>		<ip>		"1.1.1.1" .
<#vpn1>		<hasProgram>	<#vpn1666> .

@forSome <#apm1> .
<#apm1>		a <Server> .
<#apm1>		<name>		"APM1" .
<#apm1>		<ip>		"1.1.1.10" .
<#apm1>		<hasProgram>	<#cryptoapm1client80> .
<#apm1>		<hasProgram>	<#cryptoapm1client8000> .
<#apm1>		<hasProgram>	<#vpn1apm1client666> .
<#apm1>		<hasProgram>	<#cs1apm1client666> .

@forSome <#x> .
<#x>		a <Server> .
<#x>		<name>		"X" .
<#x>		<ip>		"3.1.1.1" .
<#x>		<hasProgram>	<#cs1vpnclient666> .
<#x>		<hasProgram>	<#sp1client22> .
<#x>		<hasProgram>	<#sp1client199> .
<#x>		<hasProgram>	<#sp1client514> .


@forSome <#S1> .
<#S1>		a <Server> .
<#S1>		<name>		"S1" .
<#S1>		<ip>		"2.1.1.1" .
<#S1>		<hasProgram>	<#s1client1414> .

#
#nets
#

@forSome <#net1> .
<#net1>		a <Subnet> .
<#net1>		<name>		"NET1" .
<#net1>		<hasServer>	<#sp1> .
<#net1>		<hasServer>	<#vpn1> .
<#net1>		<hasServer>	<#cs1> .

@forSome <#net2> .
<#net2>		a <Subnet> .
<#net2>		<name>		"NET2" .
<#net2>		<hasServer>	<#S1> .


@forSome <#netA> .
<#netA>		a <Subnet> .
<#netA>		<name>		"NETA" .
<#netA>		<hasServer>	<#apm1> .

@forSome <#net3> .
<#net3>		a <Subnet> .
<#net3>		<name>		"NET3" .
<#net3>		<hasServer>	<#x> .

