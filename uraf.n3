<#sp1client199>	<name>		"client to 199" .
<#sp1client199>	<communicateWith>	<#199sp1> .

@forSome <#sp1client22> .
<#sp1client22>	a <Program> .
<#sp1client22>	<name>		"client to 22" .
<#sp1client22>	<communicateWith>	<#22sp1> .

@forSome <#s1client1414> .
